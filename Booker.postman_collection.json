							"    pm.expect(bookingId).to.not.be.null;\r",
							"});\r",
							"\r",
							"pm.test(\"Verifying Booking FirstName\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var firstName = jsonData.booking.firstname;\r",
							"    \r",
							"    pm.environment.set(\"Firstname\",firstName);\r",
							"    pm.expect(firstName).to.not.be.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Verifying Booking LastName\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var lastName = jsonData.booking.lastname;\r",
							"    \r",
							"    pm.environment.set(\"Lastname\",lastName);\r",
							"    pm.expect(lastName).to.not.be.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Verifying Booking TotalPrice\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var totalPrice = jsonData.booking.totalprice;\r",
							"    \r",
							"    pm.environment.set(\"TotalPrice\",totalPrice);\r",
							"    pm.expect(totalPrice).to.not.be.null;\r",
							"});"
